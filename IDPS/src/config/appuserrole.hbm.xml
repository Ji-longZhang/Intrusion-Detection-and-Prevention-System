<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.doublegaurd.bean.UserRoleBean" table="appuserrole">
       
       <id name="uesrRoleId" type="java.lang.Integer">
            <column name="uesrRoleId" />
            <generator class="increment" />
        </id>
       
        <property name="applicationId" type="java.lang.Integer">
            <column name="applicationId" />
        </property>
       
        <property name="userRole" type="string">
            <column name="userRole" length="45" />
        </property>
        
    </class>
</hibernate-mapping>
